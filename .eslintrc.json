{
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": 2017,
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true
    }
  },
  "rules": {
    "no-console": "off"
  },
  "globals": {
    "requestAnimationFrame": true,
    "sessionStorage": true
  },
  "env": {
    "browser": true,
    "es6": true,
    "amd": true,
    "commonjs": true
  }
}
